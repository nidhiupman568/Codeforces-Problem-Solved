#include <bits/stdc++.h>
using namespace std;
#define int long long
#define ff first
#define ss second
#define pii pair<int,int>
#define vi vector<int>
#define vii vector<pii>
#define pb push_back
#define all(v) v.begin(), v.end()
#define s(v) sort(all(v))
#define el endl
int mod = 1e9 + 7;
long long binpow(long long a, long long b){if (b == 0){return 1;}else{long long ans = binpow(a, b / 2);if (b & 1){return (((ans * ans) % mod) * a) % mod;}else{return (ans * ans) % mod;}}}
static bool comp(int a,int b){return a>b;}
int gcd(int a,int b){return (b==0)?a:gcd(b,a%b);}
bool check(vector<vector<char>> &v){
    int n=v.size();
    int m=v[0].size();
    for(int i=0;i<n;i++){
        int c=0;
        for(int j=0;j<m;j++){
            if(v[i][j]=='U'){
                c++;
            }
        }
        if(c&1){
            return false;
        }
    }
    for(int i=0;i<m;i++){
        int c=0;
        for(int j=0;j<n;j++){
            if(v[j][i]=='L'){
                c++;
            }
        }
        if(c&1){
            return false;
        }
    }
    return true;
}
signed main(){
ios::sync_with_stdio(0);cin.tie(0);cout.tie(0);
    int t;
    cin>>t;
    int t1=0;
    while(t1!=t){
        t1++;
        int n,m;
        cin>>n>>m;
        vector<vector<char>> v(n,vector<char>(m));
        vector<vector<char>> ans(n,vector<char>(m,'.'));
        for(int i=0;i<n;i++){
            string s;
            cin>>s;
            for(int j=0;j<m;j++){
                v[i][j]=s[j];
            }
        }
        if(!check(v)){
            cout<<-1<<endl;
        }
        else{
            for(int i=0;i<n;i++){
                int c=0;
                for(int j=0;j<m;j++){
                    if(v[i][j]=='U' and ans[i][j]=='.'){
                        c++;
                        if(c%2==0){
                            ans[i][j]='B';
                            ans[i+1][j]='W';
                        }
                        else{
                            ans[i][j]='W';
                            ans[i+1][j]='B';
                        }
                    }
                }
            }
            // for(auto it:ans){
            //     for(auto itr:it){
            //         cout<<itr<<" ";
            //     }
            //     cout<<endl;
            // }
            // cout<<endl;
            for(int i=0;i<m;i++){
                int c=0;
                for(int j=0;j<n;j++){
                    if(v[j][i]=='L' and ans[j][i]=='.'){
                        c++;
                        if(c%2==0){
                            ans[j][i]='B';
                            ans[j][i+1]='W';
                        }
                        else{
                            ans[j][i]='W';
                            ans[j][i+1]='B';
                        }
                    }
                }
            }
            for(int i=0;i<n;i++){
                string deeps="";
                for(int j=0;j<m;j++){
                    deeps+=ans[i][j];
                }
                cout<<deeps<<endl;
            }
        }

        
    }
return 0;
}
