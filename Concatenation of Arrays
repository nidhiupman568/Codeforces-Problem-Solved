int main() {
    harderahhh;
    cin >> t;
    while (t--) {
        cin >> n;
        pair<ll, pair<ll,ll>> agi[n+5];
        for (i = 1; i <= n; i++) {
            cin >> agi[i].ss.ff >> agi[i].ss.ss;
            agi[i].ff = agi[i].ss.ff+agi[i].ss.ss;
        }
        sort (agi+1, agi+n+1);
        for (i = 1; i <= n; i++) {
            cout << agi[i].ss.ff << " " << agi[i].ss.ss << " ";
        }
        cout << endl;
    }
    
    
    return 0;
}

