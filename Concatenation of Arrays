ll x, l, y, t, z, hp, sus1, sus2, g, sus, sus3, bruh, bruh1, bruh2, bruh3, dog1, h, dog2, dog3, dog, i, p, j, q, m, a[696969], b[696969], c[696969], x1[696969], x2[696969], n, k, d[696969], joy[1005][1005];
