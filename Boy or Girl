#include <bits/stdc++.h>
using namespace std;

int main()
{
    string n; cin >> n;
    set<char> names;
    for (int i = 0; i <n.size() ; i++)
    {
        char c = n[i];
        names.insert(c);
    }
if (names.size() % 2 == 0) cout << "CHAT WITH HER!";
else cout << "IGNORE HIM!";



        return 0;
}

    		  	      	  	 		   	 		
