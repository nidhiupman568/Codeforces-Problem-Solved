#include <bits/stdc++.h>
using namespace std;
#define ff first
#define ss second
#define pb push_back
#define ll long long
int n;
 
void tc(){
    int k;
    cin >> n >> k;
    string mat[n];
    for(int i = 0 ; i < n ; i ++){
        cin >> mat[i];
    }
    for(int i = 0 ; i < n ; i += k){
        for(int j = 0 ; j < n ; j += k){
            cout << mat[i][j];
        }
        cout << "\n";
    }
 
 
 
 
}
 
void terminar_caso(){
 
}
 
 
int main(){
    #ifdef LOCAL
    freopen("in.txt", "r", stdin);
    //freopen("out.txt", "w", stdout);
 
    #endif // LOCAL
    ios::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    int t;
    cin>>t;
    while(t--){
        tc();
        terminar_caso();
    }
 
}
 
