
#include<bits/stdc++.h>
constexpr int mod=998244353,N=5010;int C[N][N];void upd(int&a,int b){a+=b-mod,a+=a>>31&mod;}void init(int n){for(int i=0;i<=n;i++){C[i][0]=1;for(int j=1;j<=i;j++){C[i][j]=(C[i-1][j]+C[i-1][j-1])%mod;}}}int main(){std::ios::sync_with_stdio(false),std::cin.tie(0);int T;init(5e3);for(std::cin>>T;T;T--){int n;std::cin>>n;std::vector<int>a(n);std::vector<int>cnt(2*n+1);for(auto&x:a)std::cin>>x,cnt[x+n]++;std::vector<std::array<int,2>>f(n+3),g(n+3);cnt[n]++;int lim=2*n;while(!cnt[lim])lim--;f[cnt[lim]+(lim>n)][1]=1;f[cnt[lim]+(lim>n)-1][0]=1;for(int i=lim-1;i>=-1;i--){int k=~i?cnt[i]:0;g.assign(n+3,std::array<int,2>{0,0});for(int j=0;j<=k;j++){int way1=1ll*f[j][1]*(j==0?k==0:C[k-1][j-1])%mod;int way0=1ll*f[j][0]*(j==0?k==0:C[k-1][j-1])%mod;upd(g[k-j+(i>n)][0],way0);upd(g[k-j+(i>n)][0],way1);upd(g[k-j+(i>n)+1][1],way1);}std::swap(f,g);}std::cout<<f[0][0]<<'\n';}return 0;}
