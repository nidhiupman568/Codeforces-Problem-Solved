#include <iostream>
#include <algorithm> 
#include <cstring> 
#include <vector> 
#include <iomanip>
#include <cmath> 
#include <deque>
#include <queue>
#include <stack>
#include <map>
using namespace std;
// typedef long long ll;
#define int long long 
void solve()
{
    int a, b, m;
    cin >> a >> b >> m;
    if(a > b)
        swap(a, b);
    int x = m + a;
    int y = m + b;
    int ans = 0;
    // cout << x << '\n';
    ans = x / a + y / b;
    cout << ans << '\n';
}
signed main()
{
    cin.tie(0);cout.tie(0);ios::sync_with_stdio(0);
    int _;
    cin >> _;
    while(_--)
    {
        solve();
    }
    return 0;
}
