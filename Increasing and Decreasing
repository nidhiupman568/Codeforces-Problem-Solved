#include<bits/stdc++.h>
//////////////////////////////////////////////
using namespace std;
////////////////////////////////////////////////

void solve(){
    /////////////////////////////////////////
    int qaz, wsx, edc; 
    ////////////////////////////////////////////////
    cin >> qaz >> wsx >> edc;
    ///////////////////////////////////////////
    
    vector<int> rfb;
    //////////////////////////////////////////////////
    int plmjk = 1;
    //////////////////////////////////////////////////
    for(int yhn = 0; yhn < edc; yhn++){
        ///////////////////////////////////////////
        rfb.push_back(wsx);
        /////////////////////////////////////////
        wsx -= plmjk;
        ///////////////////////////////////////////
        plmjk++;
        ///////////////////////////////////////////////
    }
    ///////////////////////////////////////////////////////
    
    ///////////////////////////////////////////////
    if(rfb[rfb.size()-1] < qaz) 
    ///////////////////////////////////////////
        cout << -1 << endl;
        /////////////////////////////////////////
    else{
        /////////////////////////////////////////////////
        cout << qaz << " ";
        ////////////////////////////////////////////////////
        for(int yhn = rfb.size()-2; yhn >= 0; yhn--)
        //////////////////////////////////////////////////
            cout << rfb[yhn] << " \n"[yhn == 0];
            /////////////////////////////////////////////
    }
    /////////////////////////////////////////////////
}
////////////////////////////////////////////

//////////////////////////////////////////////
int main(){
    ////////////////////////////////////////////////
    int plmjk; 
    /////////////////////////////////////////////
    cin >> plmjk;
    ///////////////////////////////////////////////////
    
    while(plmjk--) 
    /////////////////////////////////////////////////
        solve();
        //////////////////////////////////////////
        
        //////////////////////////////////////////
    return 0;
    ////////////////////////////////////
}

/////////////////////////////////////////////////////
